  "transcriptLines": {
    "1": {
      "id": "1",
      "index": 1,
      "speakerName": "Fred",
      "text": "and now I'll just click over here...",
      "textPhonetics": "ænd naʊ aɪl dʒʌst klɪk ˈoʊvər hɪər...",
      "timestamp": 2,
      "duration": 3030
    },
    "2": {
      "id": "2",
      "index": 2,
      "speakerName": "Mary",
      "text": "You look like a complete buffoon",
      "textPhonetics": "ju lʊk laɪk ə kəmˈpliːt bəˈfuːn",
      "timestamp": 6,
      "duration": 2377
    },
    "3": {
      "id": "3",
      "index": 3,
      "speakerName": "Fred",
      "text": "This buffoon has a brand new VR headset!",
      "textPhonetics": "ðɪs bəˈfuːn hæz ə brænd njuː VR ˈhɛdsɛt!",
      "timestamp": 9,
      "duration": 2298
    },
    "4": {
      "id": "4",
      "index": 4,
      "speakerName": "Mary",
      "text": "You don't know what you're doing, do you?",
      "textPhonetics": "ju doʊnt noʊ wɑt jʊər ˈduːɪŋ, du ju?",
      "timestamp": 13.5,
      "duration": 2115
    },
    "5": {
      "id": "5",
      "index": 5,
      "speakerName": "Fred",
      "text": "It just takes a little bit of getting used to",
      "textPhonetics": "ɪt dʒʌst teɪks ə ˈlɪtl bɪt əv ˈgɛtɪŋ juzd tu",
      "timestamp": 16.5,
      "duration": 2194
    },
    "6": {
      "id": "6",
      "index": 6,
      "speakerName": "Mary",
      "text": "Why on earth would you spend a fortune on this mask if you don't know how to use it?",
      "textPhonetics": "waɪ ɒn ɜrθ wʊd ju spɛnd ə ˈfɔrʧʊn ɒn ðɪs mæsk ɪf ju doʊnt noʊ haʊ tu juz ɪt?",
      "timestamp": 21,
      "duration": 4780
    },
    "7": {
      "id": "7",
      "index": 7,
      "speakerName": "Fred",
      "text": "It's an investment love",
      "textPhonetics": "ɪts ən ˈɪnvɛstmənt lʌv",
      "timestamp": 27,
      "duration": 1280
    },
    "8": {
      "id": "8",
      "index": 8,
      "speakerName": "Mary",
      "text": "This is for young people Fred",
      "textPhonetics": "ðɪs ɪz fɔr jʌŋ ˈpiːpl frɛd",
      "timestamp": 30,
      "duration": 2507
    },
    "9": {
      "id": "9",
      "index": 9,
      "speakerName": "Fred",
      "text": "I am living the future!",
      "textPhonetics": "aɪ æm ˈlɪvɪŋ ðə ˈfjuːtʃər!",
      "timestamp": 33,
      "duration": 1671
    },
    "10": {
      "id": "10",
      "index": 10,
      "speakerName": "Mary",
      "text": "You look like a ground controller in distress",
      "textPhonetics": "ju lʊk laɪk ə graʊnd kənˈtroʊlər ɪn dɪˈstrɛs",
      "timestamp": 36,
      "duration": 2899
    }
  }
